    "def zero_one():\n",
    "    import numpy as np\n",
    "    n=int(input(\"matrix dims\"))\n",
    "    matr=np.ones((n,n))\n",
    "    matr[1:n-1,1]=0\n",
    "    matr[1:n-1,n-2]=0\n",
    "    matr[1,2:n-1]=0\n",
    "    matr[n-2,2:n-1]=0\n",
    "    return matr"
